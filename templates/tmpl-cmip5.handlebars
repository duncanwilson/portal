<h2>Global Climate Model - CMIP5 Data Selector</h2>
<div id="task_submit">
  <form class="col-sm-8" id="cmip5_form" onsubmit="return task_submit();">
      <div style="display:none">
          <input type="hidden" name="csrfmiddlewaretoken" value="{{ csrftoken }}"/>
     </div>
    <div class="row">
         <div class="col-sm-4 form-group">
          <label for="parameter">Parameter</label>
           <select class="form-control" name="parameter" id="parameter"></select>
         </div>
        <div id="domain_div"></div>
    </div>
    <div class="row">
        <div id="experiment_div"></div>
        <div id="model_div"></div>
    </div>
    <div class="row">
        <div id="ensemble_div"></div>
    </div>
    <div class="row">
        <button type="submit" id="form_submit" class="btn btn-default pull-right">Submit Task</button>
    </div>
  </form>
  <div class="row" style="width:100%;clear:both;"></div>
  <div id="task_result"></div>
</div>
